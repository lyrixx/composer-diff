{
    "name": "lyrixx/composer-diff",
    "description": "Compare what changed in your composer.lock",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Gr√©goire Pineau",
            "email": "lyrixx@lyrixx.info"
        }
    ],
    "require": {
        "ion-bazan/composer-diff": "^1.12",
        "composer/composer": "^2.8.8",
        "symfony/var-dumper": "^7.2.3",
        "twig/twig": "^3.20",
        "symfony/twig-bridge": "^7.2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ion-bazan/composer-diff": false
        },
        "bump-after-update": true,
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
